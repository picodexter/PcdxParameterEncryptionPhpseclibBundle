<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="pcdx_parameter_encryption_phpseclib.encryption.algorithm.phpseclib.cipher.factory.3des"
            class="Picodexter\ParameterEncryptionPhpseclibBundle\Encryption\Algorithm\Phpseclib\Cipher\CipherFactory"
            public="false">
            <argument type="service" id="pcdx_parameter_encryption_phpseclib.encryption.algorithm.phpseclib.cipher.template.3des" />
        </service>

        <service id="pcdx_parameter_encryption_phpseclib.encryption.algorithm.phpseclib.cipher.factory.aes"
            class="Picodexter\ParameterEncryptionPhpseclibBundle\Encryption\Algorithm\Phpseclib\Cipher\CipherFactory"
            public="false">
            <argument type="service" id="pcdx_parameter_encryption_phpseclib.encryption.algorithm.phpseclib.cipher.template.aes" />
        </service>

        <service id="pcdx_parameter_encryption_phpseclib.encryption.algorithm.phpseclib.cipher.factory.blowfish"
            class="Picodexter\ParameterEncryptionPhpseclibBundle\Encryption\Algorithm\Phpseclib\Cipher\CipherFactory"
            public="false">
            <argument type="service" id="pcdx_parameter_encryption_phpseclib.encryption.algorithm.phpseclib.cipher.template.blowfish" />
        </service>

        <service id="pcdx_parameter_encryption_phpseclib.encryption.algorithm.phpseclib.cipher.template.3des"
            class="phpseclib\Crypt\TripleDES"
            public="false">
        </service>

        <service id="pcdx_parameter_encryption_phpseclib.encryption.algorithm.phpseclib.cipher.template.aes"
            class="phpseclib\Crypt\AES"
            public="false">
        </service>

        <service id="pcdx_parameter_encryption_phpseclib.encryption.algorithm.phpseclib.cipher.template.blowfish"
            class="phpseclib\Crypt\Blowfish"
            public="false">
        </service>

        <service id="pcdx_parameter_encryption_phpseclib.encryption.algorithm.phpseclib.cipher.template.des"
            class="phpseclib\Crypt\DES"
            public="false">
        </service>

        <service id="pcdx_parameter_encryption_phpseclib.encryption.algorithm.phpseclib.cipher.template.rc2"
            class="phpseclib\Crypt\RC2"
            public="false">
        </service>

        <service id="pcdx_parameter_encryption_phpseclib.encryption.algorithm.phpseclib.cipher.template.rc4"
            class="phpseclib\Crypt\RC4"
            public="false">
        </service>

        <service id="pcdx_parameter_encryption_phpseclib.encryption.algorithm.phpseclib.cipher.template.rijndael"
            class="phpseclib\Crypt\Rijndael"
            public="false">
        </service>

        <service id="pcdx_parameter_encryption_phpseclib.encryption.algorithm.phpseclib.cipher.template.twofish"
            class="phpseclib\Crypt\Twofish"
            public="false">
        </service>

        <service id="pcdx_parameter_encryption_phpseclib.encryption.algorithm.phpseclib.initialization_vector_generator"
            class="Picodexter\ParameterEncryptionPhpseclibBundle\Encryption\Algorithm\Phpseclib\InitializationVectorGenerator"
            public="false">
            <argument type="service" id="pcdx_parameter_encryption_phpseclib.encryption.algorithm.phpseclib.random_string_generator" />
        </service>

        <service id="pcdx_parameter_encryption_phpseclib.encryption.algorithm.phpseclib.random_string_generator"
            class="Picodexter\ParameterEncryptionPhpseclibBundle\Encryption\Algorithm\Phpseclib\RandomStringGenerator"
            public="false">
        </service>
    </services>
</container>
